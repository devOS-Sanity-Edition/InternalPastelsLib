{
	"schemaVersion": 1,
	"id": "innerpastels",
	"version": "${version}",
	"name": "InnerPastels",
	"description": "Library mod to share helper functions between Softer Pastels, Desolated Pastels, and any other future projects from asoji [probably]",
	"authors": [
		"asojidev"
	],
	"contact": {
		"homepage": "https://asoji.one/",
		"sources": "https://github.com/devOS-Sanity-Edition/InnerPastels"
	},
	"license": "MIT",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "gay.asoji.innerpastels.InnerPastels"
			}
		],
		"client": [
			{
				"adapter": "kotlin",
				"value": "gay.asoji.innerpastels.client.InnerPastelsClient"
			}
		]
	},
	"mixins": [
		"innerpastels.mixins.json"
	],
	"depends": {
		"fabricloader": "*",
		"minecraft": "*",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}